{
  "name": "ascii-art-maker",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "preinstall": "npm run build:wasm",
    "build:wasm": "emcc -lembind -O2 -s MODULARIZE=1 -s FORCE_FILESYSTEM=1 -s ENVIRONMENT='web' src/image-converter/hello_world.cpp -o src/image-converter/hello_world.js -s EXPORTED_RUNTIME_METHODS=FS && node tools/move-file.js src/image-converter/hello_world.wasm public/hello_world.wasm && node tools/disable-eslint-file.js src/image-converter/hello_world.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.6",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "typescript": "^4.7.4"
  }
}
